{
  "name": "periodic-madness",
  "version": "0.7.000",
  "title": "Periodic Madness",
  "author": "Mr.SmoothieHuman",
  "homepage": "https://discord.gg/64GKrqpyJ2",
  "factorio_version": "2.0",
  "dependencies": [
    "base >= 2.0.59",
    "! space-age >= 2.0",
    "! quality",
    "? BrimStuff >= 1.0.6",
    "? icon_badges >= 1.1.9",
    "? Milestones >= 1.4.2",
    "? Mini_Trains >= 2.0.0",
    "? elevated-rails",
    "? stack-inserters",
    "periodic-graphics-1 = 1.0.0"
  ],
  "description": "This mod aims to add every element in the periodic table to Factorio.",
  "package": {
    "sync_portal_details": true,
    "readme": "README.md",
    "faq": "faq.md",
    "prune_gallery": true,
    "gallery": [
      "gallery/**"
    ],
    "ignore": [
      "gallery/**"
    ],
    "markdown": {
      "images": "gallery",
      "strip_first_header": true
    }
  }
}